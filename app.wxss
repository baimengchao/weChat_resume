/**app.wxss**/
.container {
  height: 100vh;
  box-sizing: border-box;
  /*background-color: #edece8;*/
}
/* 页面主体内容 外包*/
.outer_box {
  width: 92vw;
  margin: 0 auto;
  font-size: 4vw;
  margin-bottom:4vw;
}
.outer_box:after {
  clear: both;
}

/* 页面顶部公共标题样式*/
.view_top_title {
  height: 10vw;
  font-size: 5vw;
  line-height: 10vw;
  text-align: center;
  letter-spacing: 4px;
  color: #000;
  margin-top: 4vw;
}
